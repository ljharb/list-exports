{
	"root": true,

	"extends": ["@ljharb/eslint-config/node/10", "@ljharb/eslint-config/tests"],

	"rules": {
		"complexity": 0,
		"func-style": 0,
		"id-length": 0,
		"max-lines-per-function": 0,
		"max-len": 0,
		"multiline-comment-style": 0,
		"no-negated-condition": 0,
	},

	"ignorePatterns": [
		"fixtures/*/project",
	],

	"overrides": [
		{
			"files": [
				"conditions.js",
				"conditions-expected.js",
			],
			"extends": ["@ljharb", "@ljharb/eslint-config/tests"],
			"rules": {
				"complexity": 0,
				"func-style": 0,
				"id-length": 0,
				"max-lines-per-function": 0,
				"no-negated-condition": 0,
			}
		},
		{
			"files": "add-fixture.mjs",
			"parserOptions": {
				"ecmaVersion": 2023,
			},
		},
		{
			"files": "resolving.js",
			"extends": "@ljharb/eslint-config/node/10",
			"rules": {
				"func-style": 0,
				"id-length": 0,
				"max-len": 0,
				"max-lines-per-function": 0,
				"max-nested-callbacks": 0,
				"sort-keys": 0,
			},
		},
	],
}
